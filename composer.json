{
    "name": "skys215/mazer-templates",
    "type": "library",
    "description": "Mazer templates for Skys215 Laravel Generator",
    "keywords": [
        "skys215",
        "laravel",
        "generator",
        "mazer",
        "templates"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "skys215",
            "email": "skys215@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "illuminate/view": "^9.0",
        "skys215/laravel-ui-mazer": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Skys215\\MazerTemplates\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Skys215\\MazerTemplates\\MazerTemplatesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
